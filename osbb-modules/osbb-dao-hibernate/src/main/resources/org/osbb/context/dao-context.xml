<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <!-- Shared data access objects -->
    <bean id="userLoginDao" class="org.osbb.domain.dao.impl.UserLoginDaoImpl"/>
    
    <!-- Shared data access objects -->
    <bean id="apartmentDao" class="org.osbb.domain.dao.impl.ApartmentDaoImpl"/>
    <bean id="buildingDao" class="org.osbb.domain.dao.impl.BuildingDaoImpl"/>
    <bean id="entranceDao" class="org.osbb.domain.dao.impl.EntranceDaoImpl"/>
    <bean id="personDao" class="org.osbb.domain.dao.impl.PersonDaoImpl"/>
    
</beans>